{
  "kind": "collectionType",
  "collectionName": "patients",
  "info": {
    "singularName": "patient",
    "pluralName": "patients",
    "displayName": "Patient"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "address": {
      "type": "text"
    },
    "guardian_name": {
      "type": "string"
    },
    "number": {
      "type": "string"
    },
    "dob": {
      "type": "date"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female",
        "Other"
      ]
    },
    "nic_number": {
      "type": "text"
    },
    "other_details": {
      "type": "text"
    },
    "users": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "patients"
    },
    "patient_visits": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::patient-visit.patient-visit",
      "mappedBy": "patient"
    },
    "patient_status": {
      "type": "enumeration",
      "enum": [
        "active",
        "inactive"
      ]
    }
  }
}
